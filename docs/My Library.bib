@article{strombergCenturyRiparianForest2010,
  title = {A Century of Riparian Forest Expansion Following Extreme Disturbance: {{Spatio-temporal}} Change in {{Populus}}/{{Salix}}/{{Tamarix}} Forests along the {{Upper San Pedro River}}, {{Arizona}}, {{USA}}},
