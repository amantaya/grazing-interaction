@article{leslieHumanDisturbanceWater1980,
  title = {Human {{Disturbance}} at {{Water Sources}} of {{Desert Bighorn Sheep}}},
  author = {Leslie, David M. and Douglas, Charles L.},
  date = {1980},
  journaltitle = {Wildlife Society Bulletin (1973-2006)},
  volume = {8},
  number = {4},
  eprint = {3781179},
  eprinttype = {jstor},
  pages = {284--290},
  issn = {0091-7648},
  abstract = {Alterations of behavior and movement of desert bighorn sheep (Ovis canadensis nelsoni) in the River Mountains, Nevada, were coincident with construction activities near the population's primary watering site. The juxtaposition of construction efforts and summer water dependence of bighorn sheep caused a significant shift in use of artificial water sources. Nine of 17 marked ewes altered their watering patterns in response to construction activities. Productivity during construction did not depart from the long-term population mean; however, lamb survival may have been affected. Responses of the River Mountain herd to construction activities were dampened by a high degree of habituation to man.}
}

