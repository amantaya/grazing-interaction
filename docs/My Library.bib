@article{pradoHowPerformOvariectomy,
  title = {How to Perform Ovariectomy through a Colpotomy},
  author = {Prado, T. and Schumacher, J.},
  journaltitle = {Equine Veterinary Education},
  volume = {0},
  number = {0},
  issn = {2042-3292},
  doi = {10.1111/eve.12801},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/eve.12801},
  urldate = {2018-08-06},
  abstract = {Bilateral ovariectomy of mares is performed most commonly to eliminate or diminish unwanted behaviour, create a teaser mare, sterilise a mare so that it can be registered in its breed association, or produce a recipient mare for embryo transfer. A practical technique of bilateral ovariectomy that can be easily performed without the mare sedated and without a surgical facility is ovariectomy through a colpotomy. Ovariectomy through a colpotomy is less expensive than ovariectomy using other approaches because it is performed with the mare standing, can be performed quickly and the only specialised instrument required is a chain écraseur. Complications of ovariectomy performed through a colpotomy are uncommon when the mare is correctly prepared for the procedure and when proper precautions are taken during and after the procedure.},
  langid = {english},
  keywords = {colpotomy,horse,ovariectomy},
  file = {C\:\\Users\\andre\\Zotero\\storage\\6THRPHEG\\Prado and Schumacher - How to perform ovariectomy through a colpotomy.pdf;C\:\\Users\\andre\\Zotero\\storage\\ANGRFV6D\\eve.html}
