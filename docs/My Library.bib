@article{zhangLongtermProcessbasedContinuous2021,
  title = {Long-Term, Process-Based, Continuous Simulations for a Small, Nested Rangeland Watershed near {{Tombstone}}, {{AZ}} ({{USA}}): {{Extending}} Model Validity to Include Soil Redistribution},
  shorttitle = {Long-Term, Process-Based, Continuous Simulations for a Small, Nested Rangeland Watershed near {{Tombstone}}, {{AZ}} ({{USA}})},
  author = {Zhang, Han and Renschler, Chris S. and Nichols, Mary H. and Nearing, Mark A.},
  date = {2021-10-20},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
  volume = {792},
  pages = {148403},
  issn = {0048-9697},
  doi = {10.1016/j.scitotenv.2021.148403},
  url = {https://www.sciencedirect.com/science/article/pii/S0048969721034744},
  urldate = {2022-01-11},
  abstract = {Soil or sediment redistribution prediction along hillslopes and within small watersheds is considered to be a great challenge for the application of watershed erosion models in predicting the impact of soil and water conservation measures as well as for the redistribution of pollution such as radioactive fallout. In this study, long-term soil loss and deposition were estimated for two nested semi-arid watersheds within the Walnut Gulch Experimental Watershed in Southeastern Arizona using the process-based Geo-spatial interface of WEPP (GeoWEPP). While soil parameters were previously parametrized and validated through watershed outlet runoff and sediment yields, the channel parameters were adjusted and validated based on reference values of soil redistribution generated from fallout radionuclide 137Cs samples within the watersheds. Two methods were applied for the soil redistribution analysis by comparing observed and simulated soil loss/deposition rates (a) at single pixels and reference values at the specific location of each 137Cs sample site; and (b) for average values of a 5 m radius around each 137Cs sample site to compensate for measurement and model uncertainties. Surprisingly, soil redistribution predictions improved as topographic data resolution increased from 5 m to 3 m and were best at 1 m without changing key model parameters that were originally derived at the watershed scale.},
  langid = {english},
  keywords = {Channel parameter,Deposition,GeoWEPP,Radionuclide fallout,Soil loss,Soil redistribution},
  file = {C\:\\Users\\andre\\Zotero\\storage\\SKWMYQJT\\Zhang et al. - 2021 - Long-term, process-based, continuous simulations f.pdf;C\:\\Users\\andre\\Zotero\\storage\\5NQ7L3BJ\\S0048969721034744.html}
}

