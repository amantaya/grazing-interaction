@article{snymanDynamicsSustainableUtilization1998,
  title = {Dynamics and Sustainable Utilization of Rangeland Ecosystems in Arid and Semi-Arid Climates of Southern {{Africa}}},
  author = {Snyman, H. A.},
  date = {1998-08-01},
  journaltitle = {Journal of Arid Environments},
  shortjournal = {Journal of Arid Environments},
  volume = {39},
