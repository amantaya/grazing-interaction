  langid = {english},
  keywords = {biocontrol,Coccyzus americanus,communities,Diorhabda,Empidonax traillii extimus,grasslands,middle rio-grande,nesting success,new-mexico,physiognomy,reproductive success,riparian birds,saltcedar,Tamarix,valley,vegetation},
  annotation = {WOS:000254194900017}
}

