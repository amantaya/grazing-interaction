@article{arnoldComparisonTimeBudgets1984,
  title = {Comparison of the Time Budgets and Circadian Patterns of Maintenance Activities in Sheep, Cattle and Horses Grouped Together},
  author = {Arnold, G. W.},
  year = {1984},
  month = nov,
  journal = {Applied Animal Behaviour Science},
  volume = {13},
  number = {1},
  pages = {19--30},
  issn = {0168-1591},
  doi = {10.1016/0168-1591(84)90048-0},
  urldate = {2021-09-29},
  abstract = {Sixteen 24-h periods of observation of the maintenance behaviour of sheep, cattle and horses were made when the three species grazed together in a 15-ha paddock of annual pastures in a Mediterranean environment. Observations were made in winter, spring and summer, at 15-min intervals, of the proportion of animals of each species that were grazing, feeding on supplements, standing or lying down. Time spent grazing ranged from 4.1 to 16.0 h in horses, from 2.3 to 12.7 h in cattle and 4.4 to 10.6 h in sheep. Time spent feeding (grazing + eating supplements) was strongly negatively correlated with time spent standing in horses and cattle, but only weakly so in sheep. Grazing times in winter-spring varied with pasture availability in cattle and horses, but not in sheep. Horses achieved long grazing times by grazing extensively at night, whereas sheep and cattle achieved them by intensifying their grazing during morning and afternoon grazing periods. Horses spent very little time lying down (1.1 h), whereas sheep and cattle averaged 11.6 and 10.5 h, respectively. The circadian patterns of activity were generally similar in sheep and cattle on most days; on a few others they were quite dissimilar. Those of horses were usually quite different f from those of either sheep or cattle.},
  langid = {english},
  file = {C\:\\Users\\andre\\Zotero\\storage\\V3GW2SVR\\Arnold - 1984 - Comparison of the time budgets and circadian patte.pdf;C\:\\Users\\andre\\Zotero\\storage\\NB72RMGQ\\0168159184900480.html}
}

