@misc{WSAREQuickGuide,
  title = {{{WSARE Quick Guide}}: {{Selecting Cattle}} to {{Improve Grazing Distribution}}},
  shorttitle = {{{WSARE Quick Guide}}},
  journal = {SARE Western},
