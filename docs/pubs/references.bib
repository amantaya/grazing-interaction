@article{berryAttributesDesertTortoise2006,
  title = {Attributes of Desert Tortoise Populations at the {{National Training Center}}, {{Central Mojave Desert}}, {{California}}, {{USA}}},
  author = {Berry, K.H. and Bailey, T.Y. and Anderson, K.M.},
  year = {2006},
  month = jan,
  journal = {Journal of Arid Environments},
  volume = {67},
  pages = {165--191},
  issn = {01401963},
  doi = {10.1016/j.jaridenv.2006.09.026},
  urldate = {2018-04-17},
  abstract = {We sampled 21 study plots for desert tortoises (Gopherus agassizii) at the National Training Center, Fort Irwin, California. Each plot was sampled once between 1997 and 2003 to obtain a snapshot of population attributes, status, and relationships between tortoise densities and human activities. Densities ranged from o1 to 28 tortoises km\`A2; overall, tortoises were uncommon to rare at 16 of the 21 plots. Tortoise densities were negatively correlated with death rates, infectious disease (mycoplasmosis), surface disturbance and trash. Health status of tortoises was correlated with some anthropogenic uses. The presence of infectious disease in tortoises was negatively correlated with distances from offices, the Ft. Irwin cantonment, and paved roads. Also, significantly more tortoises with shell disease were found on plots with current and recent military use than on plots with no history of military use. Factors contributing to or causing deaths of tortoises included vehicles, vandalism, predation, mycoplasmosis and shell diseases. Annual death rates for subadult and adult tortoises ranged from 1.9\% to 95.2\% for the 4 years preceding surveys. Deaths from anthropogenic sources were significantly correlated with surface disturbances, trash, military ordnance, and proximity to offices and paved roads\textemdash typical characteristics of military training areas.},
  langid = {english},
  file = {C\:\\Users\\andre\\Zotero\\storage\\285YHRNN\\Berry et al. - 2006 - Attributes of desert tortoise populations at the N.pdf}
}

