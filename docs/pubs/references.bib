  title = {Niche Partitioning among Mule Deer, Elk, and Cattle: {{Do}} Stable Isotopes Reflect Dietary Niche?},
  shorttitle = {Niche Partitioning among Mule Deer, Elk, and Cattle},
  author = {Stewart, Kelley M. and Bowyer, R. Terry and Kie, John and Dick, Brian L. and {Ben-David}, Merav},
