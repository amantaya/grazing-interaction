@article{kovacsSalivaryCortisolNoninvasive2021,
  title = {Salivary Cortisol as a Non-Invasive Approach to Assess Stress in Dystocic Dairy Calves},
  author = {Kov{\'a}cs, Levente and K{\'e}z{\'e}r, Fruzsina Luca and Bod{\'o}, Szil{\'a}rd and Ruff, Ferenc and Palme, Rupert and Szenci, Ott{\'o}},
  year = {2021},
