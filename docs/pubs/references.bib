@article{watsonPredationStronglyLimits2022,
  title = {Predation Strongly Limits Demography of a Keystone Migratory Herbivore in a Recovering Transfrontier Ecosystem},
  author = {Watson, Fred and Becker, Matthew S. and Smit, Daan and Droge, Egil and Mukula, Teddy and Martens, Sandra and Mwaba, Shadrach and Christianson, David and Creel, Scott and Brennan, Angela and M'soka, Jassiel and Gaylard, Angela and Simukonda, Chuma and Nyirenda, Moses and Mayani, Bridget},
  year = {2022},
  journal = {Ecology and Evolution},
  volume = {12},
  number = {10},
  pages = {e9414},
  issn = {2045-7758},
  doi = {10.1002/ece3.9414},
  urldate = {2022-10-24},
  abstract = {Large herbivore migrations are imperiled globally; however the factors limiting a population across its migratory range are typically poorly understood. Zambia's Greater Liuwa Ecosystem (GLE) contains one of the largest remaining blue wildebeest (Connochaetes taurinus taurinus) migrations, yet the population structure, vital rates, and limiting factors are virtually unknown. We conducted a long-term demographic study of GLE wildebeest from 2012 to 2019 of 107 collared adult females and their calves, 7352 herd observations, 12 aerial population surveys, and concurrent carnivore studies. We applied methods of vital rate estimation and survival analysis within a Bayesian estimation framework. From herd composition observations, we estimated rates of fecundity, first-year survival, and recruitment as 68\%, 56\%, and 38\% respectively, with pronounced interannual variation. Similar rates were estimated from calf-detections with collared cows. Adult survival rates declined steadily from 91\% at age 2 years to 61\% at age 10 years thereafter dropping more sharply to 2\% at age 16 years. Predation, particularly by spotted hyena, was the predominant cause of death for all wildebeest ages and focused on older animals. Starvation only accounted for 0.8\% of all unbiased known natural causes of death. Mortality risk differed substantially between wet and dry season ranges, reflecting strong spatio-temporal differences in habitat and predator densities. There was substantial evidence that mortality risk to adults was 27\% higher in the wet season, and strong evidence that it was 45\% higher in the migratory range where predator density was highest. The estimated vital rates were internally consistent, predicting a stable population trajectory consistent with aerial estimates. From essentially zero knowledge of GLE wildebeest dynamics, this work provides vital rates, age structure, limiting factors, and a plausible mechanism for the migratory tendency, and a robust model-based foundation to evaluate the effects of potential restrictions in migratory range, climate change, predator\textendash prey dynamics, and poaching.},
  langid = {english},
  keywords = {ðŸš©,demography,migration,population,predatorâ€“prey,survival,wildebeest,Zambia},
  note = {e9414 ECE-2020-08-01305.R2},
  file = {C\:\\Users\\andre\\Zotero\\storage\\WPSH9ZI8\\Watson et al_2022_Predation strongly limits demography of a keystone migratory herbivore in a.pdf}
}

